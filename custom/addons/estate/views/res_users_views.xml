<?xml version="1.0"?>
<odoo>
    <record id="estate_users_view_form" model="ir.ui.view">
        <field name="name">estate.res.users.form</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <page name="preferences" position="after">
                <page string="Propiedades real state">
                    <field name="property_ids" domain="[('state','not in',['sold','canceled'])]"/>
                </page>
            </page>
        </field>
    </record>
</odoo>